<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="form.track_time.class">Ui\SharedBundle\Form\TrackTimeType</parameter>
        <parameter key="form_handler.track_time.class">Ui\SharedBundle\Form\Handler\TrackTimeFormHandler</parameter>
    </parameters>

    <services>

        <service id="form.track_time" class="%form.track_time.class%">
            <tag name="form.type" alias="track_time" />
        </service>

        <service id="form_handler.track_time" class="%form_handler.track_time.class%">
            <argument type="service" id="command_handler.track_time"/>
        </service>

    </services>

</container>