<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="time_slot_repository.in_memory.class">CoreDomainBundle\Repository\InMemoryTimeSlotRepository</parameter>
    </parameters>

    <services>
        <!-- Exposed Services -->
        <service id="time_slot_repository" alias="time_slot_repository.in_memory"></service>

        <!-- Concrete Implementations -->
        <service id="time_slot_repository.in_memory" public="false" class="%time_slot_repository.in_memory.class%"></service>
    </services>

</container>